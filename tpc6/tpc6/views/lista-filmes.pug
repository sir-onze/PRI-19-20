extends layout

block content
    .w3-container
        h1 Lista de Filmes
        table.w3-table-all
            tr
                th Identificador
                th Title
                th Year
                th Cast
                th Genre
            each filme in lista
                tr
                    td= filme._id
                    td= filme.title
                    td= filme.year  
                    td= filme.cast
                    td= filme.genres    
                    